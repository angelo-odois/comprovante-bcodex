{
  "name": "comprovante-m53",
  "description": "Sistema de Geração de Comprovantes M-53",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".",
    "args": {}
  },
  "deploy": {
    "port": 80,
    "healthcheck": {
      "path": "/health",
      "interval": 30,
      "timeout": 10,
      "retries": 3
    }
  },
  "environment": [
    {
      "key": "NODE_ENV",
      "value": "production"
    },
    {
      "key": "DATABASE_URL",
      "value": "${DATABASE_URL}",
      "required": true
    }
  ],
  "volumes": [],
  "networks": []
}